# -*-perl-*-
######################################################################
# 
# File: t2h_standalone.init init file for generation of standalone
# html files
#

$print_page_foot = \&T2H_STANDALONE_print_page_foot;
# footer of page
sub T2H_STANDALONE_print_page_foot
{
  my $fh = shift;
  print $fh <<EOT;
<font size="-1">
&nbsp;<a href="http://www.singular.uni-kl.de/"><i>Singular</i></a> version $value{VERSION}, $value{VERSION_DATE},
generated by <a href="$Texi2HTML::THISDOC{'program_homepage'}"><i>$Texi2HTML::THISDOC{'program'}</i></a>.
</font>
$PRE_BODY_CLOSE
</body>
</html>
EOT
}

1; 	# This must be the last line
